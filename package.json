{
  "name": "@caldwell619/data-loader",
  "version": "0.0.1",
  "description": "De-duping data loader for fetching relationships",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "eslint --ext .tsx,.ts,.js src",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src && prettier --write tests",
    "pretest": "tsc --noEmit",
    "test:unit": "jest unit --detectOpenHandles",
    "test:integration": "jest integration --detectOpenHandles",
    "test:graphql": "jest integration/graphql --detectOpenHandles",
    "test:all": "jest",
    "build": "microbundle --target node --compress --tsconfig tsconfig.build.json"
  },
  "dependencies": {
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^16.1.0",
    "ioredis": "^4.28.2",
    "jest": "^27.4.4",
    "microbundle": "^0.13.3",
    "prettier": "^2.5.1",
    "rc": "^1.2.8",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.3"
  },
  "main": "dist/index.js",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "license": "MIT"
}
