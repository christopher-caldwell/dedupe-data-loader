{
  "name": "@cladwell619/data-loader",
  "version": "0.0.1",
  "description": "De-duping data loader for fetching relationships",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "eslint --ext .tsx,.ts,.js src",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src && prettier --write tests",
    "test": "jest",
    "build": "tsc --p ./tsconfig.build.json && tscpaths -p tsconfig.json -s ./src -o dist"
  },
  "dependencies": {
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "rc": "^1.2.8",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "main": "index.js",
  "source": "src/index.ts",
  "license": "MIT"
}
