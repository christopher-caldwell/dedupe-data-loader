    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0"
  }
}
